<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <!-- regular -->
    <font
        android:font="@font/dosis_regular"
        android:fontStyle="normal"
        android:fontWeight="400"

        app:font="@font/dosis_regular"
        app:fontStyle="normal"
        app:fontWeight="400" />


    <!-- extralight -->
    <font
        android:font="@font/dosis_extralight"
        android:fontStyle="normal"
        android:fontWeight="100"

        app:font="@font/dosis_extralight"
        app:fontStyle="normal"
        app:fontWeight="100" />


    <!-- light -->
    <font
        android:font="@font/dosis_light"
        android:fontStyle="normal"
        android:fontWeight="300"

        app:font="@font/dosis_light"
        app:fontStyle="normal"
        app:fontWeight="300" />

    <!-- semi-bold -->
    <font
        android:font="@font/dosis_semibold"
        android:fontStyle="normal"
        android:fontWeight="600"

        app:font="@font/dosis_semibold"
        app:fontStyle="normal"
        app:fontWeight="600" />

    <!-- bold -->
    <font
        android:font="@font/dosis_bold"
        android:fontStyle="normal"
        android:fontWeight="700"

        app:font="@font/dosis_bold"
        app:fontStyle="normal"
        app:fontWeight="700" />

    <!-- extrabold -->
    <font
        android:font="@font/dosis_extrabold"
        android:fontStyle="normal"
        android:fontWeight="900"

        app:font="@font/dosis_extrabold"
        app:fontStyle="normal"
        app:fontWeight="900" />
</font-family>